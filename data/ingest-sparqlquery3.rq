# All Antions with an English Wikipedia page and a picture
SELECT ?imageurl ?depicts ?depictsLabel ?depictsWikipediaEN
WHERE
{
  ?depicts wdt:P31 wd:Q5 . # human
  ?depicts wdt:P735 wd:{firstnameMale}}.
  ?depicts wdt:P18 ?imageurl
  FILTER (REGEX(STR(?imageurl), "\\.(jpg|png|gif)$", "i"))
  ?depictsWikipediaEN schema:about ?depicts;
          schema:isPartOf <https://en.wikipedia.org/>.
  SERVICE wikibase:label { bd:serviceParam wikibase:language "nl". }
}